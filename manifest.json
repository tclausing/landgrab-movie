{
  "name": "LandGrab Movie",
  "description": "Makes a movie out of the playback history.",
  "version": "1.0",
  "permissions": [
    "activeTab", "tabs",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_title": "LandGrab Movie",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": [
      "*://landgrab.net/landgrab/HistoryPlayback"
    ],
    "css": [],
    "js": ["dom-to-image.min.js", "libwebp-0.1.3.min.js", "whammy.js", "patch-worker.js", "script.js"]
  }],
  "web_accessible_resources": [
    "worker.js"
  ],
  "manifest_version": 2
}
